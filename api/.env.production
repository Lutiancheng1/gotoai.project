# Docker Service Ports (for production)
API_PORT=3000
MONGODB_PORT=27017
REDIS_PORT=6379

# Docker Service Hosts
MONGODB_HOST=mongo
REDIS_HOST=redis

# MongoDB Configuration
MONGODB_DATABASE=ai-chat-prod
MONGODB_USERNAME=admin
MONGODB_PASSWORD=prod_mongodb_password_complex

# Redis Configuration
REDIS_PASSWORD=prod_redis_password_complex

# JWT Configuration
JWT_SECRET=your_production_jwt_secret_key_complex
JWT_EXPIRES_IN=7d

# CORS Configuration
ALLOWED_ORIGINS=https://your-domain.com,https://admin.your-domain.com

# Log Configuration
LOG_LEVEL=error

# AI Service Configuration
DIFY_API_KEY=your_production_dify_api_key
RAGFLOW_API_KEY=your_production_ragflow_api_key
FASTGPT_API_KEY=your_production_fastgpt_api_key

# Default Admin Configuration
ADMIN_EMAIL=admin@your-domain.com
ADMIN_PASSWORD=production_admin_password_complex
ADMIN_USERNAME=administrator

# Node Environment
NODE_ENV=production 