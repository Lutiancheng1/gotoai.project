# Docker Service Ports (for production)
API_PORT=3333
MONGODB_EXTERNAL_PORT=27018
MONGODB_PORT=27017
REDIS_EXTERNAL_PORT=6389
REDIS_PORT=6379

# Docker Service Hosts
MONGODB_HOST=mongo
REDIS_HOST=redis

# MongoDB Configuration
MONGODB_DATABASE=ai-chat-prod
MONGODB_USERNAME=admin
MONGODB_PASSWORD=prod_mongodb_password_complex

# Redis Configuration
REDIS_PASSWORD=prod_redis_password_complex

# JWT Configuration
JWT_SECRET=your_production_jwt_secret_key_complex
JWT_EXPIRES_IN=7d

# Log Configuration
LOG_LEVEL=error

# Default Admin Configuration
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123
ADMIN_USERNAME=admin

# Node Environment
NODE_ENV=production 